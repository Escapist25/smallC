int main0=-913;
int a[5]={0,1,2,3,4};

int main()
{
	int i,j;
	int b[2][3];
	read(main0);
	for(i=0;i<2;++i)
	{
		for(j=0;j<3;++j)
		{
			int r;
			read(r);
			b[i][j] = r;
		}
	}
	for(i=0;i<5;++i)
	{
		int j=1000000;
		write(j);
		write(a[i]);
	}
	for(i=0;i<2;++i)
	{
		for(j=0;j<3;++j)
		{
			write(b[i][j]);
		}
	}
	return 0;
}
